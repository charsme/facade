{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"],
      "@/*": ["./src/*"],
      "~~/*": ["./*"],
      "@@/*": ["./*"]
    }
  },
  "include": [
    "./src/**/*",
    "./node_modules/cypress",
    "cypress/**/*"
  ],
  "exclude": ["node_modules", ".nuxt", "dist"]
}
